{
  "name": "electron-react-typescript-webpack-boilerplate",
  "version": "2.0.2",
  "description": "Pre-configured boilerplate for Electron + React + TypeScript",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64"
  },
  "build": {
    "appId": "com.devtography.electron_boilerplate",
    "productName": "Electron+React+TypeScript Boilerplate",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "pkg"
    },
    "win": {
      "target": "nsis",
      "asar": false
    },
    "buildVersion": "1.0.0"
  },
  "author": "Gillariel",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@material-ui/core": "^4.9.4",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.44",
    "@types/js-yaml": "^3.12.2",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.2",
    "axios": "^0.21.1",
    "clsx": "^1.1.0",
    "electron-store": "^5.1.1",
    "js-yaml": "^3.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-mason": "^1.1.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.2",
    "acorn": "^7.1.0",
    "ajv": "^6.12.0",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "electron": "^8.0.1",
    "electron-builder": "^22.3.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.15",
    "mocha": "^5.2.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "spectron": "^10.0.1",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.8.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "boilerplate",
    "Electron",
    "React",
    "Typescript",
    "Webpack"
  ],
  "bugs": {
    "url": "https://lily.xyz/issues"
  },
  "homepage": "https://lily.xyz/"
}
